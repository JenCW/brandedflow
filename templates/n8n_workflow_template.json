{
  "name": "Branded Flow - DOE Architecture Workflow",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "hours",
              "hoursInterval": 1
            }
          ]
        }
      },
      "id": "schedule-trigger",
      "name": "Schedule Trigger",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "operation": "executeWorkflow",
        "workflowId": "crewai-execution-workflow"
      },
      "id": "execute-crewai",
      "name": "Execute CrewAI Agent",
      "type": "n8n-nodes-base.executeWorkflow",
      "typeVersion": 1,
      "position": [450, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "check-success",
              "leftValue": "={{ $json.status }}",
              "rightValue": "success",
              "operator": {
                "type": "string",
                "operation": "equals"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "check-result",
      "name": "Check Execution Result",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [650, 300]
    },
    {
      "parameters": {
        "operation": "create",
        "resource": "database",
        "databaseId": "workflow-log",
        "tableId": "execution-log",
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "timestamp": "={{ $now }}",
            "status": "={{ $json.status }}",
            "result": "={{ JSON.stringify($json.result) }}",
            "errors": "={{ JSON.stringify($json.errors) }}"
          }
        },
        "options": {}
      },
      "id": "log-success",
      "name": "Log Success",
      "type": "n8n-nodes-base.database",
      "typeVersion": 1,
      "position": [850, 200]
    },
    {
      "parameters": {
        "operation": "create",
        "resource": "database",
        "databaseId": "workflow-log",
        "tableId": "error-log",
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "timestamp": "={{ $now }}",
            "status": "={{ $json.status }}",
            "error": "={{ JSON.stringify($json.error) }}",
            "context": "={{ JSON.stringify($json.context) }}"
          }
        },
        "options": {}
      },
      "id": "log-error",
      "name": "Log Error",
      "type": "n8n-nodes-base.database",
      "typeVersion": 1,
      "position": [850, 400]
    },
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "webhook",
        "options": {}
      },
      "id": "webhook-trigger",
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1.1,
      "position": [250, 500],
      "webhookId": "crewai-webhook"
    },
    {
      "parameters": {
        "operation": "executeWorkflow",
        "workflowId": "error-handler-workflow"
      },
      "id": "handle-error",
      "name": "Handle Error",
      "type": "n8n-nodes-base.executeWorkflow",
      "typeVersion": 1,
      "position": [1050, 400]
    },
    {
      "parameters": {
        "resource": "database",
        "operation": "get",
        "databaseId": "workflow-log",
        "tableId": "execution-log",
        "filters": {
          "conditions": [
            {
              "fieldName": "timestamp",
              "condition": "after",
              "value": "={{ $now.minus({ hours: 24 }) }}"
            }
          ]
        }
      },
      "id": "get-recent-executions",
      "name": "Get Recent Executions",
      "type": "n8n-nodes-base.database",
      "typeVersion": 1,
      "position": [1050, 200]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "check-error-rate",
              "leftValue": "={{ $json.errorCount }}",
              "rightValue": 5,
              "operator": {
                "type": "number",
                "operation": "gt"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "check-error-rate",
      "name": "Check Error Rate",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [1250, 200]
    },
    {
      "parameters": {
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Authorization",
              "value": "Bearer {{ $env.SLACK_TOKEN }}"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"channel\": \"#automation-alerts\",\n  \"text\": \"High error rate detected in CrewAI execution\",\n  \"attachments\": [\n    {\n      \"color\": \"danger\",\n      \"fields\": [\n        {\n          \"title\": \"Error Count\",\n          \"value\": \"={{ $json.errorCount }}\",\n          \"short\": true\n        }\n      ]\n    }\n  ]\n}",
        "options": {}
      },
      "id": "send-alert",
      "name": "Send Alert",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1450, 300]
    }
  ],
  "connections": {
    "Schedule Trigger": {
      "main": [
        [
          {
            "node": "Execute CrewAI Agent",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Execute CrewAI Agent": {
